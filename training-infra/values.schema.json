{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "rayClusterName": {
        "type": "string"
      },
      "jupyter": {
        "type": "object",
        "properties": {
          "token": {
            "type": "string"
          }
        },
        "required": ["token"]
      },
      "minio": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "user": {
            "type": "string"
          }
        },
        "required": ["name", "user"]
      },
      "environment": {
        "type": "string"
      }
    },
    "required": [
      "rayClusterName",
      "environment"
    ],
    "additionalProperties": false
  }
  