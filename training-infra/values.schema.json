{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "rayClusterName": {
      "type": "string"
    },
    "environment": {
      "type": "string"
    },
    "elasticsearch": {
      "type": "object",
      "properties": {
        "port": {
          "type": "integer",
          "minimum": 1
        }
      },
      "required": ["port"]
    }
  },
  "required": [
    "rayClusterName",
    "environment",
    "elasticsearch"
  ],
  "additionalProperties": false
}
